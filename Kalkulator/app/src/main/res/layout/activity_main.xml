<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.sample.foo.samplecalculator.MainActivity">

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button4"
        android:layout_alignParentStart="true"
        android:text="1"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="186dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button1"
        android:layout_alignBottom="@+id/button1"
        android:layout_toEndOf="@+id/button1"
        android:text="2"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="186dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button2"
        android:layout_alignBottom="@+id/button2"
        android:layout_toEndOf="@+id/button2"
        android:text="3"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="186dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button5"
        android:layout_alignBottom="@+id/button5"
        android:layout_alignStart="@+id/button9"
        android:text="4"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="250dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button6"
        android:layout_alignBottom="@+id/button6"
        android:layout_toEndOf="@+id/button9"
        android:text="5"
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="249dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button10"
        android:layout_toEndOf="@+id/button5"
        android:text="6"
        tools:layout_editor_absoluteX="201dp"
        tools:layout_editor_absoluteY="249dp" />

    <Button
        android:id="@+id/buttonSUB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/buttonDot"
        android:layout_alignBottom="@+id/buttonDot"
        android:layout_alignParentEnd="true"
        android:text="-"
        tools:layout_editor_absoluteX="293dp"
        tools:layout_editor_absoluteY="249dp" />

    <Button
        android:id="@+id/buttonADD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button11"
        android:layout_alignBottom="@+id/button11"
        android:layout_alignParentEnd="true"
        android:text="+"
        tools:layout_editor_absoluteX="293dp"
        tools:layout_editor_absoluteY="186dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button10"
        android:layout_alignBottom="@+id/button10"
        android:layout_toStartOf="@+id/button10"
        android:text="7"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="320dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button0"
        android:layout_alignStart="@+id/button0"
        android:text="8"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="320dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button10"
        android:layout_alignBottom="@+id/button10"
        android:layout_toEndOf="@+id/button10"
        android:text="9"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="320dp" />

    <Button
        android:id="@+id/buttonEQ"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="357dp"
        android:layout_height="41dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:text="="
        tools:layout_editor_absoluteY="454dp" />

    <Button
        android:id="@+id/buttonDIV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonADD"
        android:layout_alignParentEnd="true"
        android:text="/"
        tools:layout_editor_absoluteX="293dp"
        tools:layout_editor_absoluteY="320dp" />

    <Button
        android:id="@+id/button0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/button2"
        android:layout_marginBottom="63dp"
        android:text="0"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="383dp" />

    <Button
        android:id="@+id/buttonMUL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonDIV"
        android:layout_alignParentEnd="true"
        android:text="*"
        tools:layout_editor_absoluteX="293dp"
        tools:layout_editor_absoluteY="383dp" />

    <Button
        android:id="@+id/buttonDot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/button0"
        android:layout_alignBottom="@+id/button0"
        android:layout_toEndOf="@+id/button0"
        android:text="."
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="383dp" />

    <Button
        android:id="@+id/buttonDEL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonMUL"
        android:layout_alignParentEnd="true"
        android:text="DEL"
        tools:layout_editor_absoluteX="205dp"
        tools:layout_editor_absoluteY="383dp" />

    <TextView
        android:id="@+id/editText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="353dp"
        android:layout_height="63dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        android:layout_width="353dp"
        android:layout_height="95dp"
        android:layout_alignEnd="@+id/buttonEQ"
        android:layout_below="@+id/editText"
        android:text="TextView" />

</RelativeLayout>
